algoritmo "LISTA 2 ATIVIDADE 1"
var
   v,m:inteiro
inicio
      escreval("Qual a velocidade do carro em km")
      leia(v)
      se (v>80) entao
      m<-v*5
      escreva("voce foi multado no valor de",m," reais")
      senao
      escreva("voce esta na velocidade permitida")
      fimse
      

fimalgoritmo