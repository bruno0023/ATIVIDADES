algoritmo "LISTA 1 ATIVIDADE 10"
var
   a,alt,larg,valor:real
inicio
      escreva("Qual largura da parede? ")
      leia(larg)
      escreva("Qual altura da parede? ")
      leia(alt)
      a<-larg*alt
      valor<-a/2
      escreval("A area da parede é ",a)
      escreval("Sera preciso de",valor," litros de tinta para pintar a parede")
fimalgoritmo